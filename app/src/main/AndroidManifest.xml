<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.megha.vsync" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="21" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <android:uses-permission
        android:name="android.permission.WRITE_EXTERNAL_STORAGE"
        android:maxSdkVersion="18" />
    <android:uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <android:uses-permission
        android:name="android.permission.READ_EXTERNAL_STORAGE"
        android:maxSdkVersion="18" />

    <application

        android:debuggable="true"

        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Welcome"
            android:label="@string/title_activity_welcome" >
        </activity>
        <activity
            android:name=".ViewContacts"
            android:label="@string/title_activity_view_contacts" >
        </activity>
        <activity
            android:name=".AddContacts"
            android:label="@string/title_activity_add_contacts" >
        </activity>
        <activity
            android:name=".SyncActivity"
            android:label="@string/title_activity_sync" >
        </activity>
        <activity
            android:name=".ViewRequests"
            android:label="@string/title_activity_view_requests" >
        </activity>
        <activity
            android:name=".LIST"
            android:label="@string/title_activity_list" >
        </activity>
        <activity
            android:name=".Editlist"
            android:label="@string/title_activity_editlist" >
        </activity>
        <activity
            android:name=".extra"
            android:label="@string/title_activity_extra" >
        </activity>
        <activity
            android:name=".more"
            android:label="@string/title_activity_more" >
        </activity>
        <activity
            android:name=".Notifications"
            android:label="@string/title_activity_notifications" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".nav_bar"
            android:label="@string/title_activity_nav_bar" >
        </activity>
        <activity
            android:name=".welcome2"
            android:label="@string/title_activity_welcome2" >
        </activity>
        <activity
            android:name=".spinner_Act"
            android:label="@string/title_activity_spinner_" >
        </activity>
    </application>

</manifest>
